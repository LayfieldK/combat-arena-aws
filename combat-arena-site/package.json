{
  "name": "combat-arena-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "node local.js",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "serve:prerender": "node static.js",
    "build:prerender": "npm run build:prod && node dist/prerender.js",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --aot --buildOptimizer --base-href /production/",
    "build:browser:serverless:dev": "ng build --base-href /dev/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod:deploy": "npm run build:prod && npm run deploy",

    "build:serverless:deploy": "npm run build:ssr:prod && npm run deploy", 
    
    "deploy": "serverless deploy",
    "build:server:prod": "ng run combat-arena-site:server && webpack --progress --colors",
    "build:server:serverless": "ng run combat-arena-site:server:serverless && webpack --progress --colors",
    "compile:server_bak": "webpack --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node local.js",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run combat-arena-site:server:production --bundleDependencies all",
    "dev:ssr": "ng run combat-arena-site:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build && ng run combat-arena-site:server",
    "build:ssr:prod": "ng build --prod && ng run combat-arena-site:server:production",
    "prerender": "ng run combat-arena-site:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.3",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/forms": "~9.1.3",
    "@angular/localize": "^9.1.3",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/platform-server": "~9.1.3",
    "@angular/router": "~9.1.3",
    "@ng-bootstrap/ng-bootstrap": "^5.1.4",
    "@ng-toolkit/serverless": "^8.1.0",
    "@ng-toolkit/universal": "^8.0.3",
    "@nguniversal/common": "9.1.0",
    "@nguniversal/express-engine": "^9.1.0",
    "angular-in-memory-web-api": "^0.9.0",
    "apollo-angular": "^1.9.1",
    "aws-serverless-express": "^3.2.0",
    "bootstrap": "^4.4.1",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "domino": "^2.1.3",
    "express": "^4.15.2",
    "guid-ts": "^1.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "apollo-angular-link-http": "^1.10.0",
    "apollo-link": "^1.2.11",
    "apollo-client": "^2.6.0",
    "apollo-cache-inmemory": "^1.6.0",
    "graphql-tag": "^2.10.0",
    "graphql": "^14.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@nguniversal/builders": "^9.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "opencollective": "^1.0.3",
    "protractor": "~5.4.0",
    "serverless": "1.40.0",
    "serverless-apigw-binary": "^0.4.4",
    "ts-loader": "4.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "webpack-cli": "^3.3.10"
  }
}
